
class Solution:
    def find(self, arr, n, x):
        i=0
        j=n-1
        a=-1
        b=-1
        while i<=j:
            if arr[i]==x:
                a=i
            i=i+1
        while j>=0:
            if arr[j]==x:
                b=j
            j=j-1
        
        return b,a  
                
